<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/main_Home_Fragment">

    <fragment
        android:id="@+id/main_Home_Fragment"
        android:name="ir.ac.semnan.golestan.app.mysemnanuni.ui.MainHomeFragment"
        android:label="fragment_main__home_"
        tools:layout="@layout/fragment_main__home_" />
    <fragment
        android:id="@+id/dashboardFragment"
        android:name="ir.ac.semnan.golestan.app.mysemnanuni.ui.DashboardFragment"
        android:label="fragment_dashbord"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_dashboardFragment_to_scoreDetailFragment"
            app:destination="@id/scoreDetailFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_foodFragment"
            app:destination="@id/foodFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
    </fragment>
    <fragment
        android:id="@+id/notifictionFragment"
        android:name="ir.ac.semnan.golestan.app.mysemnanuni.ui.NotifictionFragment"
        android:label="fragment_notifiction"
        tools:layout="@layout/fragment_notifiction" >
        <action
            android:id="@+id/action_notifictionFragment_to_scoreDetailFragment"
            app:destination="@id/scoreDetailFragment" />
        <action
            android:id="@+id/action_notifictionFragment_to_scoreDetailFragment2"
            app:destination="@id/scoreDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/scoreDetailFragment"
        android:name="ir.ac.semnan.golestan.app.mysemnanuni.ui.ScoreDetailFragment"
        android:label="fragment_score_detail"
        tools:layout="@layout/fragment_score_detail" >
        <action
            android:id="@+id/action_scoreDetailFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
        <action
            android:id="@+id/action_scoreDetailFragment_to_notifictionFragment"
            app:destination="@id/notifictionFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_scoreDetailFragment_to_dashboardFragment2"
            app:destination="@id/dashboardFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_scoreDetailFragment_to_main_Home_Fragment"
            app:destination="@id/main_Home_Fragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/foodFragment"
        android:name="ir.ac.semnan.golestan.app.mysemnanuni.ui.FoodFragment"
        android:label="fragment_food"
        tools:layout="@layout/fragment_food" >
        <action
            android:id="@+id/action_foodFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapFragment"
        android:name="ir.ac.semnan.golestan.app.mysemnanuni.ui.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" >
        <action
            android:id="@+id/action_mapFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
    </fragment>
</navigation>